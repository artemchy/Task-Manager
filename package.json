{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "formik": "^2.4.6",
    "yup": "^1.7.0",
    "zustand": "^5.0.8",
    "clsx": "^2.1.1",
    "@dnd-kit/core": "^6.3.1"
  },
  "devDependencies": {
    "tailwindcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "terser": "^5.43.1",
    "@vitejs/plugin-react": "^5.0.0",
    "vite": "^7.1.2",
    "typescript": "~5.8.3",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/parser": "^8.40.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-prettier": "^5.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "sass-embedded": "^1.90.0",
    "typescript-eslint": "^8.40.0",
    "@eslint/js": "^9.33.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write . --ignore-unknown"
    ],
    "*.{scss}": [
      "stylelint --fix",
      "prettier --write . --ignore-unknown"
    ],
    "*.{json}": [
      "prettier --write . --ignore-unknown"
    ]
  }
}
